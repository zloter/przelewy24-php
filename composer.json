{
    "name": "zloter/przelewy24-php",
    "description": "Przelewy24 PHP library",
    "type": "library",
    "license": "MIT",
    "homepage": "https://github.com/zloter/przelewy24-php",
    "authors": [
        {
            "name": "Mateusz Nastalski",
            "homepage": "https://nastalski.pl"
        }
    ],
    "require": {
        "php": ">=7.1.3",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^6.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Przelewy24\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Przelewy24\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
